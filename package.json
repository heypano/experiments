{
    "name": "heypano-web-starter-kit",
    "version": "1.0.1",
    "description": "Pano's JS (Web Dev) starter kit",
    "main": "index.js",
    "browserslist": "last 3 versions",
    "config": {
        "server": {
            "port": 3000,
            "port_COMMENT": "This is used by localtunnel and webpack server"
        }
    },
    "scripts": {
        "clean-dist": "rimraf ./dist && mkdir dist",
        "prebuild": "npm-run-all clean-dist test lint",
        "build": "babel-node buildScripts/build.js",
        "distserver": "babel-node buildScripts/distServer.js",
        "start": "npm-run-all --parallel open:src lint:watch",
        "prettier": "prettier  --write \"**/*.{js,jsx,json,scss,css,html}\"",
        "open:src": "babel-node buildScripts/srcServer.js",
        "localtunnel": "lt --port $npm_package_config_server_port --subdomain heypano",
        "localtunnel_help": "echo \"Localtunnel exposes port $npm_package_config_server_port ($$npm_package_config_server_port) at a url (https://localtunnel.github.io/www/)\"",
        "test": "echo \"No tests yet\"",
        "prebananas": "babel-node buildScripts/prebananas.js",
        "bananas": "babel-node buildScripts/bananas.js",
        "postbananas": "babel-node buildScripts/postbananas.js",
        "security_check": "npm audit --audit-level high",
        "help": "babel-node buildScripts/help.js",
        "lint": "esw --color webpack.config.* src buildScripts",
        "lint:watch": "npm run lint -- --watch",
        "generateMockData": "babel-node buildScripts/generateMockData.js",
        "prestart-mockapi": "npm run generateMockData",
        "start-mockapi": "json-server --watch src/api/db.json --port 3001"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.11.2",
        "autoprefixer": "^9.7.1",
        "babel-eslint": "^10.0.3",
        "bootstrap": "^4.3.1",
        "core-js": "^3.4.0",
        "jquery": "^3.4.1",
        "lodash": "^4.17.15",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.19.1",
        "prop-types": "^15.7.2",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "reactstrap": "^8.1.1",
        "redux": "^4.0.4",
        "redux-freeze": "^0.1.7",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "regenerator-runtime": "^0.13.3",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.0",
        "@babel/core": "^7.7.2",
        "@babel/node": "^7.7.0",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/register": "^7.7.0",
        "babel-loader": "^8.0.6",
        "chai": "^4.2.0",
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.3",
        "compression": "^1.7.4",
        "connect-history-api-fallback": "^1.6.0",
        "copy-webpack-plugin": "^5.0.5",
        "cross-env": "^6.0.3",
        "css-loader": "^3.2.0",
        "eslint": "^6.6.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-react": "^7.16.0",
        "eslint-watch": "^6.0.1",
        "express": "^4.17.1",
        "faker": "^4.1.0",
        "file-loader": "^4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "jsdom": "^15.2.1",
        "json-schema-faker": "^0.5.0-rc23",
        "json-server": "^0.15.1",
        "localtunnel": "^2.0.0",
        "mini-css-extract-plugin": "^0.8.0",
        "mocha": "^6.2.2",
        "nock": "^11.7.0",
        "node-sass": "^4.13.0",
        "npm-run-all": "^4.1.5",
        "numeral": "^2.0.6",
        "open": "^7.0.0",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.0",
        "webpack": "^4.41.2",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/heypano/web-starter-kit.git"
    },
    "keywords": [
        "js",
        "starter",
        "kit",
        "npm",
        "babel"
    ],
    "author": "Pano Papadatos (www.heypano.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/heypano/web-starter-kit/issues"
    },
    "homepage": "https://github.com/heypano/web-starter-kit#readme"
}
