{
    "name": "heypano-web-starter-kit",
    "version": "1.0.1",
    "description": "Pano's JS (Web Dev) starter kit",
    "main": "index.js",
    "config": {
        "server": {
            "port": 3000
        }
    },
    "scripts": {
        "start": "npm-run-all --parallel security_check open:src lint:watch",
        "open:src": "babel-node buildScripts/srcServer.js",
        "localtunnel": "lt --port $npm_package_config_server_port --subdomain heypano",
        "localtunnel_help": "echo \"Localtunnel exposes port $npm_package_config_server_port ($$npm_package_config_server_port) at a url (https://localtunnel.github.io/www/)\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prebananas": "babel-node buildScripts/prebananas.js",
        "bananas": "babel-node buildScripts/bananas.js",
        "postbananas": "babel-node buildScripts/postbananas.js",
        "security_check": "nsp check",
        "help": "babel-node buildScripts/help.js",
        "lint": "esw --color webpack.config.* src buildScripts",
        "lint:watch": "npm run lint -- --watch"
    },
    "dependencies": {
        "whatwg-fetch": "1.0.0"
    },
    "devDependencies": {
        "babel-cli": "6.16.0",
        "babel-core": "6.17.0",
        "babel-loader": "6.2.5",
        "babel-preset-env": "^1.6.1",
        "babel-register": "6.16.3",
        "chai": "3.5.0",
        "chalk": "1.1.3",
        "cheerio": "0.22.0",
        "compression": "1.6.2",
        "cross-env": "3.1.3",
        "css-loader": "0.25.0",
        "eslint": "3.8.1",
        "eslint-plugin-import": "2.0.1",
        "eslint-watch": "2.1.14",
        "express": "4.14.0",
        "extract-text-webpack-plugin": "1.0.1",
        "html-webpack-plugin": "2.22.0",
        "jsdom": "9.8.0",
        "json-schema-faker": "0.3.6",
        "json-server": "0.8.22",
        "localtunnel": "1.8.1",
        "mocha": "3.1.2",
        "nock": "8.1.0",
        "npm-run-all": "3.1.1",
        "nsp": "2.6.2",
        "numeral": "1.5.3",
        "open": "0.0.5",
        "rimraf": "2.5.4",
        "style-loader": "0.13.1",
        "webpack": "1.13.2",
        "webpack-dev-middleware": "1.8.4",
        "webpack-hot-middleware": "2.13.0",
        "webpack-md5-hash": "0.0.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/heypano/web-starter-kit.git"
    },
    "keywords": [
        "js",
        "starter",
        "kit",
        "npm",
        "babel"
    ],
    "author": "Pano Papadatos (www.heypano.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/heypano/web-starter-kit/issues"
    },
    "homepage": "https://github.com/heypano/web-starter-kit#readme"
}
